                <path>assignment</path>
                <display>
                    <label>
                        <t:orig>Role</t:orig>
                        <t:norm>role</t:norm>
                        <t:translation>
                            <t:key>reportUserList.roleColumn.label</t:key>
                        </t:translation>
                    </label>
                </display>
                <previousColumn>activationColumn</previousColumn>
                <export>
                    <expression>
                        <script>
                            <code>
                                if (input != null){
                                    roles = report.resolveRoles(input);
                                    if (roles.isEmpty()) {
                                        return null;
                                    }
                                    list = new ArrayList();
                                    for (role in roles) {
                                        list.add(role.getName());
                                    }
                                    return list;
                                }
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
